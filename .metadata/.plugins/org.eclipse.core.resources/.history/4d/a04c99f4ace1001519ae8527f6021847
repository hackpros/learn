package com.navigate.treat.rest.service;

import org.learn.boot.domain.Users;
import org.learn.boot.domain.UsersQueryHelper;

public interface IUsersService extends IBaseService<Users, UsersQueryHelper> {
	/**
	 * 用户注册
	 * @param user
	 * @return
	 */
	Users doRegister(Users user);
	/**
	 * 用户登录
	 * @param user
	 * @return
	 */
	Users doLogin(Users user);
	/**
	 * 验证码校对
	 * @param userloginRes
	 * @param user
	 */
	void checkCaptcha(Users user);
	/**
	 * 
	 * @param l
	 * @return
	 */
	Users selectById(long id);
    
}